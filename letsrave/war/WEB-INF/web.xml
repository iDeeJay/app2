<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<servlet>
		<servlet-name>SystemServiceServlet</servlet-name>
		<servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
		<init-param>
			<param-name>services</param-name>
			<param-value />
		</init-param>
	</servlet>

	<servlet>
		<servlet-name>ChannelCreate</servlet-name>
		<servlet-class>com.letsrave.ChannelCreateServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Channel</servlet-name>
		<servlet-class>com.letsrave.ChannelServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ChannelCreate</servlet-name>
		<url-pattern>/channel_create</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Channel</servlet-name>
		<url-pattern>/channel</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>SystemServiceServlet</servlet-name>
		<url-pattern>/_ah/spi/*</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
